<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>/List.xhtml</from-view-id>
        <navigation-case>

            <from-outcome>GoToView_product</from-outcome>

            <to-view-id>/product/View.xhtml</to-view-id>

            <redirect/>

        </navigation-case>
       
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/manager.xhtml</from-view-id>
        <navigation-case>

            <from-outcome>GoToView_product</from-outcome>

            <to-view-id>/product/View.xhtml</to-view-id>

            <redirect/>

        </navigation-case>
       
    </navigation-rule>
    <navigation-rule> 
        <from-view-id>/cart/Checkout.xhtml</from-view-id>
        <navigation-case>

            <from-outcome>index</from-outcome>

            <to-view-id>/product/List.xhtml</to-view-id>

            <redirect/>

        </navigation-case>
    </navigation-rule>
    <navigation-rule> 
        <from-view-id>/List.xhtml</from-view-id>
        <navigation-case>

            <from-outcome>gotoCart</from-outcome>

            <to-view-id>/cart/List.xhtml</to-view-id>

            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule> 
        <from-view-id>/Create.xhtml</from-view-id>
        <navigation-case>

            <from-outcome>gotoCart</from-outcome>

            <to-view-id>/cart/List.xhtml</to-view-id>

            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule> 
        <from-view-id>/edit.xhtml</from-view-id>
        <navigation-case>

            <from-outcome>gotoCart</from-outcome>

            <to-view-id>/cart/List.xhtml</to-view-id>

            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule> 
        <from-view-id>/manager.xhtml</from-view-id>
        <navigation-case>

            <from-outcome>gotoCart</from-outcome>

            <to-view-id>/cart/List.xhtml</to-view-id>

            <redirect/>
        </navigation-case>
    </navigation-rule>

<navigation-rule> 
    <from-view-id>/purchase.xhtml</from-view-id>
    <navigation-case>

        <from-outcome>gotoCart</from-outcome>

        <to-view-id>/cart/List.xhtml</to-view-id>

        <redirect/>
    </navigation-case>
</navigation-rule>
<navigation-rule> 
    <from-view-id>/cart/Checkout.xhtml</from-view-id>
    <navigation-case>

        <from-outcome>gotoList</from-outcome>

        <to-view-id>/List.xhtml</to-view-id>

        <redirect/>
    </navigation-case>
</navigation-rule>

</faces-config>
